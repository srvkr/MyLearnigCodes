
public class bubbleSorting {
    public static void main(String[] args) {

        int[] n = {1,4,2,8,0,3,9,4};
        int temp = 0;
        for(int i = 0; i<n.length-1; i++){
            for(int j = i+1; j<n.length; j++){
                if(n[i] > n[j]){
                    temp = n[i];
                    n[i] = n[j];
                    n[j] = temp;
                }
            }

        }

        for (int j =0; j<n.length; j++) {
            System.out.print(n[j]);
        }


        }

    }